{
  "name": "jgraph",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "dev:infra": "docker compose -f compose.dev.yml up -d",
    "dev:infra:down": "docker compose -f compose.dev.yml down",
    "dev:web": "cd web && bun run dev",
    "dev:server": "cd server && bun run dev",
    "dev:engine": "cd engine && cargo watch -x run",
    "db:generate": "cd server && bun run db:generate",
    "db:migrate": "cd server && bun run db:migrate",
    "db:push": "cd server && bun run db:push",
    "db:studio": "cd server && bun run db:studio",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@types/bun": "^1.3.9",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "7.0.0",
    "lefthook": "^2.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript-eslint": "^8.56.0"
  }
}
